<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TraceColor" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="InfoColor" Color="Green"/>
    <SolidColorBrush x:Key="WarnColor" Color="Orange"/>
    <SolidColorBrush x:Key="ErrorColor" Color="Red"/>
    <SolidColorBrush x:Key="FatalColor" Color="Purple"/>
</ResourceDictionary>